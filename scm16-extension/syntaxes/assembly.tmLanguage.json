{
    "scopeName": "custom.source.assembly",
    "patterns": [
        {
            "name": "custom.word1.assembly",
            "match": "^\\s*0[xX][0-9a-fA-F]+"
        },
        {
            "name": "custom.word2.assembly",
            "match": "(?<=^\\s*0[xX][0-9a-fA-F]+\\s+)0[xX][0-9a-fA-F]+"
        },
        {
            "name": "custom.word3.assembly",
            "match": "(?<=^\\s*0[xX][0-9a-fA-F]+\\s+0[xX][0-9a-fA-F]+\\s+)0[xX][0-9a-fA-F]+"
        },
        {
            "name": "custom.word4.assembly",
            "match": "(?<=\\s+)0[xX][0-9a-fA-F]+(?=\\s*#|\\s*$)"
        },
        {
            "name": "custom.comment.assembly",
            "match": "#.*"
        }
    ],
    "repository": {},
    "names": "assembly",
    "fileTypes": ["assembly"],
    "uuid": "00000000-0000-0000-0000-000000000000"
}
